      <p id="notice"><%= notice %></p>
      <h1>Zombies</h1>

    <div class="container">


      <div class="row">
        <% @zombies.each do |zombie| %>
          <div class="col-sm-6 col-md-4">
            <div class="thumbnail">
              <%= image_tag zombie.photo, alt: "zombies", class: 'img-responsive' %>
              <div class="caption">
                <h3>Nombre: <%= zombie.name %></h3>
                <p>Eliminados: <%= zombie.killed %></p>
                <p><%= zombie.killed %></p>
                <p><%= zombie.name %></p>
                <p><%= zombie.health %></p>
                <p><%= link_to 'Show', zombie %></p>
                <p><%= link_to 'Edit', edit_zombie_path(zombie) %></p>
                <p><%= link_to 'Destroy', zombie, method: :delete, data: { confirm: 'Are you sure?' } %></p>
                <p><a href="#" class="btn btn-primary" role="button">Button</a> <a href="#" class="btn btn-default" role="button">Button</a></p>
              </div>
            </div>
          </div>
        <% end%>
      </div>
    </div>



